    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "pg-query-stream": "^4.8.1",
    "pkg-pr-new": "^0.0.43",
    "prettier": "^2.8.8",
    "remap-istanbul": "^0.13.0",
    "rimraf": "^5.0.10",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "peerDependencies": {
    "pg": "^8.15.6",
    "reflect-metadata": "*",
    "snowflake-sdk": "^2.0.4"
  },
