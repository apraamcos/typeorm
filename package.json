    "@google-cloud/spanner": {
      "optional": true
    },
    "@sap/hana-client": {
      "optional": true
    },
    "hdb-pool": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mssql": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "oracledb": {
